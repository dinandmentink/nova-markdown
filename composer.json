{
    "name": "dinandmentink/nova-markdown",
    "description": "A Laravel Nova Markdown Field.",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "markdown",
        "image upload"
    ],
    "authors": [
        {
            "name": "Dinand Mentink",
            "email": "dinand@dcreative.nl",
            "homepage": "https://www.dcreative.nl",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/dinandmentink/nova-markdown",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0",
        "spatie/image": "^2.0",
        "laravel/nova": "^4.0",
        "nova-kit/nova-packages-tool": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DinandMentink\\Markdown\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DinandMentink\\Markdown\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require-dev": {
        "laravel/pint": "^1.6"
    },
    "post-update-cmd": [
        "./vendor/bin/pint"
    ]
}
